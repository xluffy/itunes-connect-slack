{
  "env": {
    "browser": false,
    "node": true
  },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"

    },
    "rules": {
      "indent": [
        "error",
        2,
        { "SwitchCase": 1  }
      ],
      "no-console": 0,
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    },
    "settings": {
    }
}

